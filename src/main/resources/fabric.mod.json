{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Platymemo"
  ],
  "contact": {
    "homepage": "https://github.com/Platymemo/fabric-example-mod",
    "sources": "https://github.com/Platymemo/fabric-example-mod",
    "issues": "https://github.com/Platymemo/fabric-example-mod/issues"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.platymemo.example.ExampleMod"
    ],
    "client": [
      "com.github.platymemo.example.ExampleModClient"
    ],
    "modmenu": [
      "com.github.platymemo.example.compat.ExampleModMenu"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "alaskanativecraft": "*"
  }
}
